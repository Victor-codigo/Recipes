{% if this.getType == 'page_main' %}

<h1 class="mb-5  pt-4  pb-2  text-left  border-bottom  text-primary  primary-color-dark  display-4">
     <span class="display-6  text-secondary">{{ data.titlePathLabel }}</span>{{ data.titleLabel }}
</h1>

{% elseif this.getType == 'pop_up' %}

<h1 class="mb-5  pt-4  pb-2  text-center  display-5">
     <span class="display-6  text-secondary">{{ data.titlePathLabel }}</span>{{ data.titleLabel }}
</h1>

{% endif %}